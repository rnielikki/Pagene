{
  "metadata": [
    {
      "src": [
        {
          "src":"../..",
          "files": "Pagene.**/**/**.csproj",
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "**/Pagene.Editor/**",
            "**/**.Tests/**"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/**.md"
        ]
      },
      {
        "files": [
          "help/**.md",
          "help/**.yml"
        ]
      },
      {
        "files": [
          "toc.yml",
          "index.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/material"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}